<div class="container">
  <h1>チーム作成</h1>
  <%= form_with model: @team, local: true do |f| %>
    <div>チーム名</div>
    <%=f.text_field :team_name %><br><br>
    <div>チーム画像</div>
    <%=f.file_field :team_img, accept: 'imag/*' %><br><br>
    <div>チームの特徴</div>
    <%=f.text_field :team_introduction %><br><br>
    <%=f.submit "作成" %>
  <%end%>
</div>
